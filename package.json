    "build": "tailwindcss -i ./assets/css/main.css -o ./assets/css/style.css && hugo --minify"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.7",
    "concurrently": "^8.0.1",
    "cypress": "^10.3.1",
    "netlify-plugin-cypress": "^2.2.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.6"
  }
